@page "/enemies"
@using Models.Enemies
@using BlazorApp.Components
@inherits EnemiesComponent
<div class="container-fluid">
    <div class="row">
        @if (Enemies == null)
        {
            <p>loading... </p>
        }
        else
        {
            @foreach (var enemy in Enemies)
            {
                <EnemyView Enemy="enemy" />
            }
        }
    </div>
</div>
